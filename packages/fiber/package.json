{
  "name": "solid-three",
  "version": "0.0.4",
  "description": "A Solid renderer for Threejs",
  "keywords": [
    "solidjs",
    "solid",
    "renderer",
    "fiber",
    "three",
    "threejs"
  ],
  "author": "Paul Henschel (https://github.com/drcmda)",
  "license": "MIT",
  "maintainers": [
    "Josh Ellis (https://github.com/joshuaellis)",
    "Cody Bennett (https://github.com/codyjasonbennett)"
  ],
  "bugs": {
    "url": "https://github.com/pmndrs/react-three-fiber/issues"
  },
  "homepage": "https://github.com/pmndrs/react-three-fiber#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmndrs/react-three-fiber.git"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-three-fiber"
  },
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "react-native": "native/dist/react-three-fiber-native.cjs.js",
  "sideEffects": false,
  "preconstruct": {
    "entrypoints": [
      "index.tsx",
      "native.tsx",
      "solid.tsx"
    ]
  },
  "source": "src/index.tsx",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/esm/index.js",
      "node": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "prebuild": "cp ../../readme.md readme.md"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "@solid-primitives/resize-observer": "^2.0.4",
    "scheduler": "^0.21.0",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "solid-js": "1.5.1",
    "rollup": "^2.78.1",
    "rollup-preset-solid": "^1.4.0"
  },
  "peerDependencies": {
    "expo": ">=46.0",
    "expo-asset": ">=8.6",
    "expo-gl": ">=11.4",
    "react": ">=18.0",
    "react-dom": ">=18.0",
    "react-native": ">=0.69",
    "solid-js": ">=1.5.0",
    "three": ">=0.141"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    },
    "expo": {
      "optional": true
    },
    "expo-asset": {
      "optional": true
    },
    "expo-gl": {
      "optional": true
    }
  }
}
